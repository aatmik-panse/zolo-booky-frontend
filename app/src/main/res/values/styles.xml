<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="gradient">
        <item name="android:background">@drawable/fade_overlay</item>
    </style>

    <style name="fadeShapeAppearanceOverlay">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="overlay">@drawable/fade_overlay</item>
    </style>


    <style name="AppTheme2" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- other theme attributes -->
        <item name="android:statusBarColor">@android:color/white</item>
    </style>

    <style name="ActiveIndicatorStyle">
        <item name="android:background">@drawable/bottomnav_colorselector</item>
        <item name="android:color">@drawable/bottomnav_colorselector</item>
        <item name="android:iconTint">@drawable/bottomnav_colorselector</item>
    </style>
    <style name="CustomButtonStyle" parent="Widget.AppCompat.Button">
        <item name="android:background">@null</item>
    </style>





</resources>